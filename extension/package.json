{
  "name": "officemonkey-extension",
  "version": "0.5.0",
  "description": "Chrome extension for OfficeMonkey calendar and contact enhancements",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "test": "vitest",
